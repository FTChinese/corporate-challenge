          <div class="o-header__logo">
            <a class="o-header__logo-link" href="http://www3.ftchinese.com/app/jpmorgan/index.html" title="{{title}}">
              <span>{{title}}</span>
              <img src="/images/cc-logo.png" target="_blank">
            </a>
          </div> 
        </div>

        <div class="o-header__right">
          <div class="o-share" data-o-component="o-share"></div>          
        </div>         
      </div>
        
    </div><!-- o-header__container -->    
